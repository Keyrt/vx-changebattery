  padding-bottom: 30rpx;
}

.input {
  margin: 120rpx auto 80rpx;
  /* position: relative;
  overflow: hidden; */
}

.iemiInput {
  width: 634rpx;
  height: 98rpx;
  border-color: rgb(227, 225, 225);
  border-width: 1px;
  border-style: solid;
  border-radius: 6px;
  font-size: 14px;
  text-align: center;
  margin: 0 auto;
  /* position: absolute;

/* 遮罩 */
.shadow {
  width: 100%;
  height: 100%;
  z-index: 80;
  position: absolute;
  top: 0;
  background-color: #000;
  opacity: 0.5;
}

/* 上滑高度 */
.choosen {
  width: 100%;
  height: 760rpx;
  position: fixed;
  bottom: 0;
  background-color: #fff;
  /* border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx; */
  z-index: 98;
}

/* 内容 */
.container-box {
  display: flex;
  height: 100%;
  flex-direction: column;
  width: 100%;
  /* border-radius: 15rpx; */
  margin: 0 auto;
  z-index: 98;
}

.logo {
  position: absolute;
  top: -80rpx;
  width: 160rpx;
  height: 160rpx;
  right: 20rpx;
}

.logo image {
  width: 100%;
  height: 100%;
}

.title-box {
  margin-top: 80rpx;
  margin-left: 20rpx;
  padding: 0 20rpx 26rpx;
}

.title-box image {
  float: left;
  width: 160rpx;
  height: 160rpx;
  border-radius: 10rpx;
}

.title-box .title {
  position: relative;
  width: 72%;
  height: 160rpx;
  float: left;
  margin-left: 20rpx;
  line-height: 44rpx;
  font-size: 32rpx;
  color: rgb(119, 119, 119);
  overflow: hidden;
}

.two {
  display: -webkit-box  !important;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.title-box .btn {
  position: absolute;
  display: table-cell;
  background-color: rgb(255, 191, 0);
  border-radius: 42rpx;
  padding: 4rpx 30rpx;
  font-size: 24rpx;
  color: #333;
  bottom: 0;
}

.container-box .item {
  padding: 16rpx;
}

.container-box .item image {
  width: 50rpx;
  height: 50rpx;
  float: left;
  margin-right: 20rpx;
}

.container-box .item text {
  display: inline-block;
  width: 90%;
  font-size: 28rpx;
  color: rgb(119, 119, 119);
  /* margin-left: 20rpx; */
}


/* 遮罩结束 */
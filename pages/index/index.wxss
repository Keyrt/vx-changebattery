.container {
  height: 100vh;
  width: 100%;
}

#batteryMap {
  height: 100%;
  width: 100%;
  box-sizing: border-box;
  padding-bottom: 96rpx+env(safe-area-inset-bottom);
}

.customCallout {
  box-sizing: border-box;
  background-color: #fff;
  border: 1px solid #ccc;
  border-radius: 30px;
  width: 150px;
  height: 40px;
  display: inline-flex;
  padding: 5px 20px;
  justify-content: center;
  align-items: center;
}

.icon {
  width: 48rpx;
  height: 48rpx;
}

.content {
  flex: 0 1 auto;
  margin: 0 10px;
  font-size: 14px;
}


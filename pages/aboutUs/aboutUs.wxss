/* pages/aboutUs/aboutUs.wxss */
.container {
  width: 100%;
  height: 100vh;
  box-sizing: border-box;
  padding: 10rpx 0;
  background-color: #FBFBFB;
  color: #333;
  position: relative;
}

.title {
  font-weight: bold;
  font-size: 46rpx;
  color: rgb(16, 16, 16);
  line-height: 80rpx;
  text-align: center;
  margin-top: 80rpx;
}

.version {
  font-size: 32rpx;
  color: rgb(16, 16, 16);
  line-height: 68rpx;
  text-align: center;
}

.introduction {
  font-weight: 400;
  font-size: 28rpx;
  color: rgb(16, 16, 16);
  line-height: 52rpx;
  box-sizing: border-box;
  padding: 0 40rpx;
  text-indent: 60rpx;
  margin-top: 88rpx;
}